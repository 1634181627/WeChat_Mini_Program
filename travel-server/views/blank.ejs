                    document.getElementById("hName_" + idValue).innerHTML = nameValue;
                    document.getElementById("hArea_" + idValue).innerHTML = areaValue;
                    document.getElementById("hImg_" + idValue).innerHTML = imgValue;
                    document.getElementById("hPrice_" + idValue).innerHTML = priceValue;
                    document.getElementById("hIntro_" + idValue).innerHTML = introValue;
                }
            }
        }
    }

    //酒店删除
    function deleteHotel(id) {
        $('#deleteHotelsId').prop({
            value: id
        })
    }

    //酒店删除保存
    function deleteHotels() {
        var Hotel3_cancel = document.getElementById('Hotel3_cancel');
        Hotel3_cancel.click();
        var id = document.getElementById('deleteHotelsId');
        idValue = id.value;
        // console.log(idValue)
        var data = "id=" + idValue;
        var http = new XMLHttpRequest();
        http.open('post', 'http://localhost:8088/deleteHotels', true);
        http.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
        http.send(data);
        http.onreadystatechange = function () {
            if (http.readyState == 4 && http.status == 200) {
                var code = JSON.parse(http.responseText).code;
                if (code == 1) {
                    alert("删除成功！");
                    document.getElementById("Hotel_" + idValue).remove();
                }

            }
        }
    }

